{
    "hosting": {
        "public": "build",
        "headers": [
            {
                "source": "**",
                "headers": [
                    {
                        "key": "Cache-Control",
                        "value": "max-age=7200"
                    }
                ]
            }
        ],
        "rewrites": [
            {
                "source": "/favicon.ico",
                "destination": "/favicon.ico"
            },
            {
                "source": "**",
                "destination": "/index.html"
            }
        ]
    }
}